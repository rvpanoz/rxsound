{
  "presets": [
    [
      "env",
      {
        "targets": { "chrome": "58" },
        "useBuiltIns": true
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": ["dev-expression"]
    },
    "development": {
      "plugins": ["transform-class-properties", "transform-es2015-classes"]
    }
  }
}
